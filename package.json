{
  "name": "droplol",
  "version": "1.2.0",
  "description": "drop.lol CLI client for easy peer-to-peer file transfers.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "file",
    "transfer",
    "peer-to-peer",
    "p2p",
    "cli",
    "typescript"
  ],
  "bin": {
    "drop": "./lib/index.js"
  },
  "scripts": {
    "start": "ts-node-dev --respawn --transpileOnly src/index.ts",
    "prepare": "npm run build",
    "test": "jest",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mat-sz/droplol.git"
  },
  "bugs": {
    "url": "https://github.com/mat-sz/droplol/issues"
  },
  "homepage": "https://github.com/mat-sz/droplol",
  "author": "mat-sz <contact@matsz.dev>",
  "license": "BSD-3-Clause-Clear",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/cli-progress": "^3.4.2",
    "@types/command-line-args": "^5.0.0",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/uuid": "^7.0.0",
    "@types/ws": "^7.2.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5",
    "webrtc": "^1.14.1"
  },
  "dependencies": {
    "cli-progress": "^3.6.0",
    "command-line-args": "^5.1.1",
    "file-type": "^14.1.3",
    "filesize": "^6.1.0",
    "matcrypt": "^1.2.1",
    "node-webcrypto-ossl": "^2.1.2",
    "typesocket": "^1.0.0",
    "uuid": "^7.0.1",
    "wrtc": "^0.4.7",
    "ws": "^7.2.1"
  }
}
